<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BookMapper">
    <sql id="selectSql">
        SELECT
    </sql>

    <select id="selectByIsbn" resultType="entity.Book">
        select
            *
        from t_book
        where
          isbn = #{isbn}
    </select>

</mapper>