<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BookStockMapper">
    <sql id="selectSql">
        SELECT
    </sql>

    <update id="updateBookStock">
        update t_book_stock
            set
            stock = stock - 1
        where
          isbn = #{isbn}
    </update>

</mapper>